#!name=策略面板
#!desc=𝗷𝘂𝗻𝗷𝗶𝗲 最后更新时间 2023-04-11
#!system=ios

[Panel]
策略面板 = script-name=𝗪𝗔𝗥𝗣信息,title="𝗪𝗔𝗥𝗣信息",content="请刷新面板",style=info,update-interval=1800
策略面板 = script-name=电信流量,title="电信流量",content="请刷新面板",style=info,update-interval=3600
策略面板 = script-name=代理选择,title="代理选择",content="请刷新面板",style=info,update-interval=1800
策略面板 = script-name=网络状态,title="网络状态",content="请刷新面板",style=info,update-interval=1800
策略面板 = script-name=流量统计,title="流量统计",content="请刷新面板",style=info,update-interval=1800

[Script]
# > 策略面板
𝗪𝗔𝗥𝗣信息 = type=generic,timeout=15,script-path=https://raw.githubusercontent.com/VirgilClyne/Cloudflare/main/js/Cloudflare.1.1.1.1.panel.js,script-update-interval=0,argument=Title=𝗪𝗔𝗥𝗣信息&Icon=checkmark.seal&IconColor=#f48220&Language=auto
电信流量= type=generic,timeout=10,script-path= https://github.com/ChinaTelecomOperators/ChinaTelecom/releases/download/Prerelease-Alpha/Tele_Cellular_Panel.js,argument=icon=simcard.2&color=#5A36C4
代理选择 = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/fishingworld/something/main/groupPanel.js,argument=icon=bolt.badge.a.fill&color=#bacf65&group=𝗣𝗥𝗢𝗫𝗬
网络状态 = type=generic,timeout=10,script-path=https://raw.githubusercontent.com/HentaiUltra/Script/main/JavaSrcipt/NetInfoPanel.js,argument=icon=wifi.circle&color=#1ba784
流量统计 = type=generic,timeout=10,script-path= https://raw.githubusercontent.com/fishingworld/something/main/PanelScripts/trafficstatistics.js,argument=icon=arrow.up.arrow.down&color=#6e8b74

